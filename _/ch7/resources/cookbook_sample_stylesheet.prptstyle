<?xml version="1.0" encoding="UTF-8"?>
<style-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/style/1.0" xmlns:layout="http://reporting.pentaho.org/namespaces/engine/classic/bundle/layout/1.0" xmlns:core="http://reporting.pentaho.org/namespaces/engine/attributes/core" xmlns:html="http://reporting.pentaho.org/namespaces/engine/attributes/html" xmlns:swing="http://reporting.pentaho.org/namespaces/engine/attributes/swing" xmlns:pdf="http://reporting.pentaho.org/namespaces/engine/attributes/pdf" xmlns:designtime="http://reporting.pentaho.org/namespaces/report-designer/2.0" xmlns:crosstab="http://reporting.pentaho.org/namespaces/engine/attributes/crosstab" xmlns:pentaho="http://reporting.pentaho.org/namespaces/engine/attributes/pentaho" xmlns:table="http://reporting.pentaho.org/namespaces/engine/attributes/table" xmlns:page="http://jfreereport.sourceforge.net/namespaces/reports/legacy/ext">
  <rule>
    <selector>.details-header</selector>
    <styles>
      <border-styles border-top-width="1" border-left-width="1" border-bottom-width="1" border-top-color="#030000" border-left-color="black" border-bottom-color="black" border-top-style="solid" border-left-style="solid" border-bottom-style="solid"/>
    </styles>
  </rule>
  <rule>
    <selector>.details-header-last</selector>
    <styles>
      <border-styles border-width="1" border-color="black" border-style="solid"/>
    </styles>
  </rule>
  <rule>
    <selector>.details-row-cell</selector>
    <styles>
      <border-styles border-left-width="1" border-bottom-width="1" border-left-color="black" border-bottom-color="black" border-left-style="solid" border-bottom-style="solid"/>
    </styles>
  </rule>
  <rule>
    <selector>.details-row-cell-last</selector>
    <styles>
      <border-styles border-left-width="1" border-bottom-width="1" border-right-width="1" border-left-color="black" border-bottom-color="black" border-right-color="black" border-left-style="solid" border-bottom-style="solid" border-right-style="solid"/>
    </styles>
  </rule>
</style-definition>
